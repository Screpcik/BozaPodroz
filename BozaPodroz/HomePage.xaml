<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:BozaPodroz"
             x:Class="BozaPodroz.HomePage">

    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="Nowy"
                     Icon="add.png"
                     Clicked="ToolbarItem_Clicked"/>
    </TabbedPage.ToolbarItems>
    <local:HistoryPage Title="Historia"
                       Icon="home.png"/>
    <local:MapPage Title="Mapy"
                   Icon="map.png"/>
    <local:ProfilePage Title="Profil"
                       Icon="user.png"/>
</TabbedPage>